<%= simple_form_for model, method: method do |f| %>
  <%= f.input :coursename %><br />
  <%= f.input :description %><br />
  <%= f.input :date_of_end %><br />
  <%= f.input :locale, as: :radio_buttons, collection: Education::VALID_LOCALES, item_wrapper_class: 'inline' %>
  <%= f.button :submit, submit, class: "btn btn-outline-dark my-2 btn-sm" %> 
  <%= render 'shared/cancel_button' if f.object.persisted? %>     
<% end %>
